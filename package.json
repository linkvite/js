{
  "name": "@linkvite/js",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/node/index.js",
        "require": "./dist/node/index.cjs"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linkvite/js.git"
  },
  "homepage": "https://docs.linkvite.io/sdks/server/js",
  "author": "Linkvite <support@linkvite.io>",
  "license": "MIT",
  "sideEffects": false,
  "packageManager": "pnpm@7.28.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "rm -rf dist && tsup && node ./build/copy.js",
    "bench": "LINKVITE_DEBUG=true tsx _workbench.ts",
    "release": "pnpm build && pnpm changeset publish",
    "fullbuild": "pnpm build && pnpm docs",
    "test": "tsx tests/index.ts",
    "package": "pnpm build && pnpm pack",
    "publish:pkg": "pnpm build && pnpm publish --access public",
    "publish:dev": "pnpm build && npm link"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "18.15.5",
    "dotenv": "16.0.3",
    "prettier": "2.8.6",
    "tsup": "8.0.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  },
  "description": "ðŸ¤  Javascript SDK for Linkvite. Requires Node.js 12+. Inspired by [Hop](https://github.com/hopinc/js)",
  "bugs": {
    "url": "https://github.com/linkvite/js/issues"
  },
  "directories": {
    "example": "example"
  },
  "keywords": [
    "bookmark-manager",
    "bookmarks"
  ]
}
